<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE visualization SYSTEM "../../visualization.dtd">
<visualization name="Chromatin Interactions">
    <data_sources>
        <data_source>
            <model_class>HistoryDatasetAssociation</model_class>
            <test type="isinstance" test_attr="datatype" result_type="datatype">binary.Mrh</test>
            <to_param param_attr="id">dataset_id</to_param>
        </data_source>
    </data_sources>
    <params>
        <param type="dataset" var_name_in_template="hda" required="true">dataset_id</param>

        <param type="int" default="0">start1</param>
        <param type="int" default="0">start2</param>
        <param type="int" default="1000000">window_size</param>
        <param type="int" default="1000001">min_resolution</param>
        <param type="int" default="3907">max_resolution</param>
        <param type="float" default="-10.0">expected_min</param>
        <param type="float" default="10.0">expected_max</param>

        <param>region</param>
    </params>
    <template>mrheatmap.mako</template>
</visualization>
